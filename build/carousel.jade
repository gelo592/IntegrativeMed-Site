#myCarousel.carousel.slide(data-ride="carousel")
  ol.carousel-indicators
    li.active(data-target="#myCarousel", data-slide-to="0")
    li(data-target="#myCarousel", data-slide-to="1")
    li(data-target="#myCarousel", data-slide-to="2")
  .carousel-inner(role="listbox")
    .item.slide.active
      img.image-responsive.center-block(src="img/carousel-1-min.jpg")
      .carousel-caption
        .info-blob
          .blob
            p.blob-title Hours:
            p.blob-info Monday-Friday
            p.blob-info 7:30am-4pm
    .item.slide
      img.image-responsive.center-block(src="img/carousel-2-min.jpg")
      .carousel-caption
        .info-blob
          .blob
            p.blob-title: a.blob-glyph(href="https://goo.gl/maps/6lP2I") Location:
            p.blob-info-sm 999 Diamond Ridge Dr
            p.blob-info-sm Suite 201
            p.blob-info-sm Jefferson City, MO
            p.blob-info-sm 65109
    .item.slide
      img.image-responsive.center-block(src="img/carousel-3-min.jpg")
      .carousel-caption
        .info-blob
          .blob
            p.blob-title Contact:
            p.blob-info-sm (573) 632-5585
            p.blob-info-sm Call for appointments
            p.blob-info-sm or for a supplement refill.
  a.left.carousel-control(href="#myCarousel", role="button" data-slide="prev")
    span.glyphicon.glyphicon-chevron-left(aria-hidden="true")
    span.sr-only Previous
  a.right.carousel-control(href="#myCarousel", role="button" data-slide="next")
    span.glyphicon.glyphicon-chevron-right(aria-hidden="true")
    span.sr-only Next
